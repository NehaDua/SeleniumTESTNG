<?xml version="1.0"?>
<Object_Repository>
	<Object Name="UST_Page" Type="Browser">
		<Def Type="ParentObject">
			<Description />
		</Def>
		<Object Name="userName" Type="Browser">
			<Def Type="ChildObject">
				<Description>
					<xpath>//*[@id='login-form-username']</xpath>
				</Description>
			</Def>
		</Object>
		<Object Name="userPassword" Type="Browser">
			<Def Type="ChildObject">
				<Description>
					<xpath>//*[@id='login-form-password']</xpath>
				</Description>
			</Def>
		</Object>
		<Object Name="loginButton" Type="Browser">
			<Def Type="ChildObject">
